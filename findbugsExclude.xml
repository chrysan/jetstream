<?xml version="1.0"?>

<FindBugsFilter>
	
	<!-- This is to ignore the immutable field error -->
     <Match>
       <Bug code="BC,EI,EI2,UrF" />
     </Match>
     
     <!-- ConfigWeb project will be removed from Raptor and hence removing it from analysis -->
     <Match>
       <Class name="~.*Test$"/>
     </Match>
     <Match>
       <Package name="~test\..*"/>
     </Match>

     <Match>
       <Package name="~tests\..*"/>
     </Match>

</FindBugsFilter>
